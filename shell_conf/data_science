# open jupyter notebook
ojn() {
    if [ $# -eq 0 ]; then
        echo "Usage: ojn <notebook files path>"
        return
    fi

    if [ ! -d "$1" ]; then
        echo "Usage: ojn <notebook files path>"
        return
    fi

    docker run -it -d --rm -p 8888:8888 -v $1:/home/jovyan jupyter/datascience-notebook start-notebook.sh --NotebookApp.token=''
}
